<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.itsmart.profileMnt.dao.SampleDAO">
    <select id="selectInfo" resultType="kr.co.itsmart.profileMnt.vo.SampleVO">
        <![CDATA[
        select EMPNO as empNo
             , ENAME as empName
             , HIREDATE as hireDate
             , DEPTNO as deptNo
        from emp
        limit 1
        ]]>
    </select>
</mapper>
